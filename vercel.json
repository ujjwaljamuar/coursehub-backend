{
    "version": 2,
    "name": "coursehub-backend",
    "builds": [{ "src": "./server.js", "use": "@vercel/node" }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/",
            "headers": {
                "Access-Control-Allow-Origin": "https://coursehub-frontend.vercel.app",
                "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE"
            }
        }
    ]
}